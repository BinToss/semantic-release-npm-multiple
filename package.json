{
  "name": "@bintoss/semantic-release-npm-multiple",
  "version": "1.0.0",
  "description": "A semantic release plugin to publish an NPM package to multiple registries.",
  "main": "index.js",
  "repository": "https://github.com/bintoss/semantic-release-npm-multiple",
  "author": "Amanda Mitchell <she@mandymitchell.me>",
  "contributors": [
    "Noah 'bintoss' Sherwin <noahrsherwin@gmail.com>"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "npm",
    "publish",
    "registry",
    "semantic-release",
    "version"
  ],
  "scripts": {
    "postinstall": "husky",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "node --test"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "^25.0.10",
    "commitlint": "^20.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "pinst": "^3.0.0",
    "prettier": "^3.0.2",
    "semantic-release": "^25.0.2"
  },
  "dependencies": {
    "@semantic-release/npm": "^13.0.0"
  },
  "peerDependencies": {
    "semantic-release": ">=19.0.0"
  }
}
